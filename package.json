{
  "name": "a5cforge",
  "private": true,
  "packageManager": "pnpm@9.15.4",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "eslint": "^9.17.0",
    "eslint-plugin-sonarjs": "^2.0.4",
    "jscpd": "^4.0.5"
  },
  "scripts": {
    "build": "pnpm -r --if-present build",
    "test": "pnpm -r --if-present build && pnpm -r test",
    "coverage": "pnpm -r --if-present build && pnpm -r --if-present test:coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dupcheck": "jscpd --config .jscpd.json"
  }
}


