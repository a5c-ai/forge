{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://a5cforge.dev/schemas/a5cforge-v1/agent.progress.created.schema.json",
  "title": "a5cforge/v1 - agent.progress.created",
  "allOf": [
    { "$ref": "./event.base.schema.json" },
    {
      "type": "object",
      "properties": {
        "kind": { "const": "agent.progress.created" },
        "payload": {
          "type": "object",
          "additionalProperties": true,
          "required": ["dispatchId", "agentId"],
          "properties": {
            "dispatchId": { "type": "string", "minLength": 1 },
            "agentId": { "type": "string", "minLength": 1 },
            "percent": { "type": "number", "minimum": 0, "maximum": 100 },
            "message": { "type": "string" }
          }
        }
      }
    }
  ]
}


